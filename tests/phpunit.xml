<phpunit bootstrap="./Bootstrap.php">
    <testsuite name="Glicko2 Test Suite">
        <directory>./</directory>
    </testsuite>

    <groups>
        <exclude>
            <group>disable</group>
        </exclude>

    </groups>

    <filter>
        <whitelist>
            <directory suffix=".php">library/</directory>
        </whitelist>
    </filter>
</phpunit>
